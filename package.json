{
  "name": "webvim",
  "version": "0.8.0",
  "description": "",
  "main": "dist/elm.js",
  "scripts": {
    "css": "node ./build/less.config.js",
    "start": "node ./start.js && npm run start",
    "purs": "pulp build -o dist --src-path src-purs --test-path tests-purs",
    "purs:release": "pulp build -O -o dist --src-path src-purs --to dist/webvim-backend.js",
    "purs:watch": "pulp -w run -o dist --src-path src-purs",
    "js": "elm make src/Main.elm --output dist/elm.js --debug --warn",
    "release": "npm run css && node ./build/release.js",
    "html": "node ./build/html.config.js",
    "eslint": "eslint start.js build/*.js src-purs/**/*.js --fix"
  },
  "author": "chendesheng",
  "license": "MIT",
  "devDependencies": {
    "ajv": "^5.0.0",
    "browser-sync": "^2.24.4",
    "chokidar": "^2.0.3",
    "clean-css": "^4.1.11",
    "debounce": "^1.1.0",
    "eslint": "^4.19.1",
    "eslint-config-google": "^0.9.1",
    "execa": "^0.10.0",
    "google-closure-compiler-js": "^20180402.0.0",
    "hasha": "^3.0.0",
    "husky": "^0.15.0-rc.13",
    "less": "^2.7.3",
    "less-plugin-autoprefix": "^1.5.1",
    "less-plugin-clean-css": "^1.5.1",
    "webfonts-generator": "^0.4.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free-webfonts": "^1.0.9",
    "comment-json": "^1.1.3",
    "diff": "^3.5.0",
    "hapi": "^17.4.0",
    "memorystream": "^0.3.1",
    "vscode-textmate": "^3.3.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run eslint && elm format src/ --yes && elm format tests/ --yes && elm test && npm run release"
    }
  }
}
