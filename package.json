{
  "name": "webvim",
  "version": "0.4.0",
  "description": "",
  "main": "dist/elm.js",
  "scripts": {
    "css": "node ./build/less.config.js",
    "start": "node ./start.js && npm run start",
    "js": "elm make src/Main.elm --output dist/elm.js --debug --warn",
    "release": "npm run css && node ./build/release.js",
    "html": "node ./build/html.config.js",
    "syntaxserver": "node ./src-js/textmate.js"
  },
  "author": "chendesheng",
  "license": "MIT",
  "devDependencies": {
    "browser-sync": "^2.24.4",
    "chokidar": "^2.0.3",
    "clean-css": "^4.1.11",
    "debounce": "^1.1.0",
    "execa": "^0.8.0",
    "google-closure-compiler-js": "^20180402.0.0",
    "hasha": "^3.0.0",
    "husky": "^0.15.0-rc.13",
    "less": "^2.7.3",
    "less-plugin-autoprefix": "^1.5.1",
    "less-plugin-clean-css": "^1.5.1",
    "webfonts-generator": "^0.4.0"
  },
  "dependencies": {
    "comment-json": "^1.1.3",
    "hapi": "^17.4.0",
    "vscode-textmate": "^3.3.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "elm format src/ --yes && elm format tests/ --yes && elm test && npm run release && ./fake"
    }
  }
}
